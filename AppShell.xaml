<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ActivitatiVoluntariatMOBILE.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ActivitatiVoluntariatMOBILE"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            Title="Meniu"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" 
            />

        <ShellContent
            Title="Despre"
            ContentTemplate="{DataTemplate local:DesprePage}"
            Route="DesprePage"
            />

    </TabBar>
    

</Shell>
